{
  "executable": "Concoct-Builder",
  "splashscreen": {
    "imageFile": "rokonocontrol.png",
    "width": 500,
    "height": 500
  },
  "name": "Concoct-Builder",
  "author": "Collaborativesoft",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.Concoct-Builder.app",
    "productName": "Concoct-Builder",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "ConcoctBuilder.db",
      "**/*",
      "rokonocontrol.png"
    ]
  }
}